{
  "engines": {
    "node": ">=8"
  },
  "main": "app.js",
  "private": true,
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "app.js"
        ],
        "msg": "Minesweeper - Node.js on Google Cloud Platform"
      },
      "build": {
        "requiredEnvVars": [
          "DB_USER",
          "DB_PASS",
          "DB_NAME",
          "CLOUD_SQL_CONNECTION_NAME"
        ]
      }
    }
  },
  "scripts": {
    "start": "node app.js",
    "cover": "nyc --cache npm test; nyc report --reporter=html"
  },
  "dependencies": {
    "bcrypt": ">= 0.8.0",
    "body-parser": ">= 1.10.1",
    "chance": ">= 0.0.3",
    "cors": ">= 2.5.2",
    "express": ">= 4.10.6",
    "morgan": ">= 1.5.1",
    "multer": ">= 0.1.6",
    "mysql": ">= 2.5.4",
    "underscore": ">= 1.7.0",
    "validator": ">= 3.27.0"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.0.0",
    "bcrypt": ">= 0.8.0",
    "body-parser": ">= 1.10.1",
    "chance": ">= 0.0.3",
    "cors": ">= 2.5.2",
    "express": ">= 4.10.6",
    "morgan": ">= 1.5.1",
    "multer": ">= 0.1.6",
    "mysql": ">= 2.5.4",
    "underscore": ">= 1.7.0",
    "validator": ">= 3.27.0"
  }
}
